<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moodcoin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/moodicon"
        android:label="Mood Coin"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme= "@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.moodcoin.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.moodcoin.MainActivity"></activity>
        <activity android:name="com.example.moodcoin.LobbyActivity"></activity>
        <activity android:name="com.example.moodcoin.TestLobbyActivity"></activity>
        <activity android:name="com.example.moodcoin.FragmentMain"></activity>
        <activity android:name="com.example.moodcoin.FragmentCamera"></activity>
        <activity android:name="com.example.moodcoin.FragmentCall"></activity>
    </application>

</manifest>
